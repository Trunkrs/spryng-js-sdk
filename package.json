    "@types/axios-mock-adapter": "^1.10.0",
    "@types/jest": "^26.0.5",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "axios-mock-adapter": "^1.18.2",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
